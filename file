pipeline {
    agent any
    stages {
        stage('git scm') {
            steps {
                echo 'this is fetching stage'
            }
        }
         stage('build') {
            steps {
                echo 'this is build stage'
            }
        }
         stage('test') {
            steps {
                echo 'this is test stage'
            }
        }
         stage('deploy') {
            steps {
                echo 'this is deploy stage'
            }
        }
    }
}




