pipeline {
    agent any
    stages {
        stage('git scm') {
            steps {
                git 'https://github.com/Dimple-sasi-12/declarative.git'
            }
        }
         stage('build') {
            steps {
                bat 'mvn install'
            }
        }
         stage('test') {
            steps {
                echo 'this is test stage'
            }
        }
         stage('deploy') {
            steps {
                echo 'this is deploy stage'
            }
        }
    }
}




