pipeline {
    agent any
    tools {
        maven 'maven'
    }
    stages {
        stage('git scm') {
            steps {
                git branch: 'main', url: 'https://github.com/Dimple-sasi-12/declarative.git'
            }
        }
         stage('build') {
            steps {
                bat 'mvn install'
            }
        }
         stage('test') {
            steps {
                echo 'this is test stage'
            }
        }
         stage('deploy') {
            steps {
                echo 'this is deploy stage'
            }
        }
    }
}





